<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef Name="Anneli_Ahmislan_1AB" ParentName="Anneli_Ahmislan_ApparelCommonBase"  Abstract="True">
		<costStuffCount>25</costStuffCount>
		<stuffCategories>
			<li>Leathery</li>
			<li>Fabric</li>
		</stuffCategories>
		<statBases>
			<WorkToMake>250</WorkToMake>
			<Mass>0.5</Mass>
			<StuffEffectMultiplierArmor>0.1</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>0.25</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0.25</StuffEffectMultiplierInsulation_Heat>
			<EquipDelay>1</EquipDelay>
			<Flammability>0.5</Flammability>
		</statBases>
		<equippedStatOffsets>
			<Cleanliness>0.10</Cleanliness>
		</equippedStatOffsets>
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Legs</li>
			</bodyPartGroups>
			<layers>
				<li>OnSkin</li>
			</layers>
			<tags>
				<li>Anneli_Ahmislan</li>
			</tags>
		</apparel>
	</ThingDef>

<!--Anneli_Ahmislan_CamisoleA-->
	<ThingDef ParentName="Anneli_Ahmislan_1AB">
		<defName>Anneli_Ahmislan_CamisoleA</defName>
		<description>Dyeable.</description><!-- \nAn camisole with a lot of lace. Not doing good for a resolution below 512. -->
		<label>(Ahmislan)1AB_CamisoleA</label>
		<equippedStatOffsets>
			<NegotiationAbility>0.2</NegotiationAbility>
			<SocialImpact>0.2</SocialImpact>
		</equippedStatOffsets>
		<graphicData>
			<texPath>1AB/Anneli_Ahmislan_CamisoleA</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<apparel>
			<wornGraphicPath>1AB/Anneli_Ahmislan_CamisoleA</wornGraphicPath>
			<tags>
				<li>Anneli_Ahmislan_CourtLady</li>
				<li>Anneli_Ahmislan_Merchant</li>
				<li>Anneli_Ahmislan_MercenaryArcher</li>
			</tags>
			<useWornGraphicMask>true</useWornGraphicMask><!--Mask Applied-->
		</apparel>
		<colorGenerator Class="ColorGenerator_Options"><!--Tansparency 225-->
			<options>
				<li>
					<weight>10</weight>
					<only>(250,240,240,225)</only>
				</li>
			</options>
		</colorGenerator>
	</ThingDef>

