<Defs>

	<ThingDef Name="Anneli_Ahmislan_34" ParentName="Anneli_Ahmislan_ApparelCommonBase"  Abstract="True">
		<costStuffCount>150</costStuffCount>
		<!-- <stuffCategories>
			<li>Leathery</li>
			<li>Fabric</li>
		</stuffCategories> -->
		<statBases>
			<WorkToMake>22000</WorkToMake>
			<Mass>10</Mass>
			<StuffEffectMultiplierArmor>0.85</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>0.5</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0.5</StuffEffectMultiplierInsulation_Heat>
			<EquipDelay>15</EquipDelay>
			<Flammability>0.15</Flammability>
		</statBases>
		<equippedStatOffsets>
			<ComfyTemperatureMin>-30</ComfyTemperatureMin> 
			<SuppressionPower>0.2</SuppressionPower>
			<PainShockThreshold>0.1</PainShockThreshold>
			<MeleeHitChance>0.1</MeleeHitChance>
			<MeleeDodgeChance>0.1</MeleeDodgeChance>
		</equippedStatOffsets>
		<apparel>
			<shellCoversHead>false</shellCoversHead>
			<shellRenderedBehindHead>true</shellRenderedBehindHead><!--Shell layer visual glitch-->
			<bodyPartGroups>
				<li>Torso</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Legs</li>
			</bodyPartGroups>
			<layers>
				<li>Anneli_UpperShell_Armor</li>
				<li>Shell</li>
			</layers>
			<tags>
				<li>Anneli_Ahmislan</li>
			</tags>
		</apparel>
	</ThingDef>

